algoritmo "antecessor_sucessor"
          var
             n: inteiro,
             a: inteiro,
             s: inteiro
          inicio
          escreval("Olá, digite um número")
          leia(n)
          a:= n-1
          s:= n+1
          escreva("O numéro digitado foi: ",n," seu antecessor é: ",a," e seu sucessor é: ", s)
fimalgoritmo